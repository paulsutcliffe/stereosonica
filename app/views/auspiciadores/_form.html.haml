= form_for @auspiciador do |f|
  - if @auspiciador.errors.any?
    #error_explanation
      %h2= "#{pluralize(@auspiciador.errors.count, "error")} prohibited this auspiciador from being saved:"
      %ul
        - @auspiciador.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :link
    = f.text_field :link
  .actions
    = f.submit 'Save'
